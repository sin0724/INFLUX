# Supabase Configuration
# 아래 값들을 실제 Supabase 프로젝트의 값으로 변경하세요

NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_service_role_key_here

# JWT Secret (랜덤 문자열, 최소 32자)
# 아래 명령어로 생성: openssl rand -base64 32
JWT_SECRET=your_random_secret_key_at_least_32_characters_long

# Encryption Key (네이버 비밀번호 등 암호화용, 64자 hex 또는 임의 문자열)
# 아래 명령어로 생성: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_KEY=your_encryption_key_64_characters_hex_or_random_string

# Node Environment
NODE_ENV=development

